datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}
generator client {
    provider = "prisma-client-js"
}

model Idea {
  id          String @id @default(uuid())
  nick        String @unique
  name        String
  description String
  text        String
  createdAt   DateTime @default(now())
}

model User {
  id String @id @default(uuid())
  nick String @unique
  password String
  createdAt DateTime @default(now())
}